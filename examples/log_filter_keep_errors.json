{
  "id": "keep-error-logs",
  "name": "Keep Error Logs",
  "description": "Only keep logs with ERROR severity level",
  "enabled": true,
  "log_filter": {
    "matchers": [
      {
        "log_severity_text": {
          "regex": "^ERROR$"
        }
      }
    ],
    "action": "FILTER_ACTION_KEEP"
  }
}
