{
  "id": "redact-ssn-in-body",
  "name": "Redact SSN in Log Body",
  "description": "Replace Social Security Numbers in log body with [SSN REDACTED]",
  "priority": 100,
  "enabled": true,
  "log_transform": {
    "matchers": [
      {
        "log_body": {
          "regex": "\\b[0-9]{3}-[0-9]{2}-[0-9]{4}\\b"
        }
      }
    ],
    "field": {
      "log_body": {}
    },
    "replacement": {
      "pattern": "\\b[0-9]{3}-[0-9]{2}-[0-9]{4}\\b",
      "replacement": "[SSN REDACTED]"
    }
  }
}
