{
  "id": "drop-password-attribute",
  "name": "Drop Password Attribute",
  "description": "Remove the user.password attribute entirely from logs",
  "priority": 100,
  "enabled": true,
  "log_transform": {
    "matchers": [
      {
        "log_attribute": {
          "key": "user.password",
          "regex": ".*"
        }
      }
    ],
    "field": {
      "log_attribute": {
        "key": "user.password"
      }
    },
    "drop": {}
  }
}
