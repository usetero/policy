{
  "id": "drop-healthcheck-logs",
  "name": "Drop Health Check Logs",
  "description": "Drop logs from health check endpoints to reduce noise",
  "enabled": true,
  "log_filter": {
    "matchers": [
      {
        "log_body": {
          "regex": "/health|/readiness|/liveness"
        }
      }
    ],
    "action": "FILTER_ACTION_DROP"
  }
}
